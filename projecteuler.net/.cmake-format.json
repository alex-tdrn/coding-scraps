{
	"format": {
		"autosort": false,
		"command_case": "canonical",
		"dangle_align": "prefix",
		"dangle_parens": true,
		"disable": false,
		"enable_sort": true,
		"fractional_tab_policy": "round-up",
		"keyword_case": "upper",
		"line_ending": "unix",
		"line_width": 120,
		"max_lines_hwrap": 2,
		"max_pargs_hwrap": 6,
		"max_prefix_chars": 10,
		"max_rows_cmdline": 2,
		"max_subgroups_hwrap": 2,
		"min_prefix_chars": 4,
		"require_valid_layout": false,
		"separate_ctrl_name_with_space": false,
		"separate_fn_name_with_space": false,
		"tab_size": 4,
		"use_tabchars": true
	}
}